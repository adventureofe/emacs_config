#include <iostream>
#include <map>
#include <memory>
#include <vector>
#include <utility>
#include <string>
#include <cmath>
#include <numbers>


// print all included cmd args (removes warning)
void arg_print(int argc, char** argv)
{
    for(int i = 1; i < argc; i++)
    {
        printf("arg[%d]: %s", i, argv[i]);
    }
    if(argc > 1)
    {
        printf("\n");
    }
}

int main(int argc, char** argv)
{
    arg_print(argc, argv);

    std::cout << "Hello, World!\n";
    return 0;
}

